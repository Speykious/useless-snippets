{
	"name": "useless-snippets",
	"icon": "smiley-neutral.png",
	"repository": "https://github.com/Speykious/useless-snippets.git",
	"displayName": "Useless Snippets",
	"description": "Basically... Useless snippets.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"languages": [
			{
				"id": "text",
				"extensions": [
					".txt"
				]
			},
			{
				"id": "xyz",
				"extensions": [
					"xyz"
				]
			}
		],
		"grammars": [
			{
				"language": "xyz",
				"scopeName": "source.xyz",
				"path": "./syntaxes/xyz.tmGrammar.json"
			}
		],
		"snippets": [
			{
				"language": "text",
				"path": "./snippets/snippets.json"
			}
		]
	},
	"main": "index.js",
